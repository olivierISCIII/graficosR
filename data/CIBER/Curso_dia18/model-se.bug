model
{
# Para cada una de las J observaciones
for(i in 1:J)
	{
	X[i] ~ dpois(mu[i])
	log(mu[i]) <- log(Y[i]) + alpha[ID[i]] + YEAR*year[i] + YEAR2*year2[i]    	
	}
# (efectO aleatoriO)
for(i in 1:18) 
	{
	alpha[i] ~ dnorm(a, tau)
	}
# Priors 
a ~ dnorm(0.0, 0.001)
YEAR ~ dnorm(0.0, 0.001) 
YEAR2 ~ dnorm(0.0, 0.001) 
# Hyperprior
tau ~ dgamma(0.01,0.01)
sigma <- 1/sqrt(tau)
}
